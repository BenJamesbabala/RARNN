%
    ~prefix? %if ~suffix?
    ~prefix? %sequence ~suffix?
    ~prefix? %action ~suffix?

%if
    if %condition then %action
    if %condition then %sequence

%sequence
    %action ~also ~please? %action
    %action ~also ~please? %action ~also ~please? %action
    %action ~also ~please? %action ~also ~please? %action ~also ~please? %action

%condition
    %checkValue
    %getNumberValue is $operator $value

%checkValue=
    %checkLightState
    %checkSwitchState

%getNumberValue=
    %getPrice
    %getTemperature

%getValue=
    %getLightState
    %getSwitchState
    %getPrice
    %getTemperature

%action
    ~get %getValue
    %setLightState
    %setSwitchState
    %setTemperature
    %setVolume
    %playMusic
    
# Specific commands
# ---------------------------------------------------------

%checkLightState
    the $light is $on_off

%checkSwitchState
    the $switch is $on_off

%getLightState
    the $light state

%getSwitchState
    the $switch state
   
%getTemperature
    the temperature in the $room
    the $room temperature

%getPrice
    price of $asset
    $asset price

%setLightState
    ~set the $light %light_state
    ~set $on_off the $light

%light_state=
    $on_off
    $up_down
    $color

%setSwitchState
    ~set the $switch $on_off
    ~set $on_off the $switch

%setTemperature
    ~set the $room temperature to $value
    ~set the temperature of $room to $value

%setVolume
    ~set the volume $up_down

%playMusic
    play $song
    play ~something_by $artist

